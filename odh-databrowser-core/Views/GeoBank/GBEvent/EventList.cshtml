        <div class="col-sm-9 col-xs-12 table-responsive">
            @*<div class="pull-right">
            <a ng-click="editevent('new')" class="btn btn-small btn-success">New</a>
        </div>
        <br />*@
            <table class="table table-striped table-condensed">
                <thead>
                    <tr>
                        <th style="width:160px">image</th>
                        <th style="min-width: 80px;">name</th>
                        <th style="min-width: 80px;">begindate</th>
                        <th style="min-width: 80px;">enddate</th>
                        <th style="min-width: 80px;">location</th>
                        <th style="min-width: 80px;">languages</th>
                        <th style="min-width: 80px;">active (source/ODH)</th>
                        <th style="min-width: 80px;">published</th>
                        <th style="width:20px;"> </th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="event in events">
                        <td>
                            <div style="max-width:150px;min-height:75px">
                                <img class="img-thumbnail img-responsive" @*style="max-height:150px;max-width:150px"*@ ng-src="{{  event.ImageGallery[0] != NULL && event.ImageGallery[0].ImageUrl + '&W=150&H=150' || '../img/noimage.png' }}" @*ng-hide="event.ImageGallery[0] == NULL"*@ title="{{ event.ImageGallery[0].ImageDesc['de']}}" ng-click="showInfoModal(event)" />
                            </div>
                        </td>
                        @*<td>{{ event.Id }}</td>*@
                        <td>{{ event.Detail[lang].Title }}</td>
                        @*<td>{{ event.EventDate[0].From | moment:'DD/MM/YYYY' }}</td>*@
                        <td>{{ event.DateBegin | moment:'DD/MM/YYYY' }}</td>
                        <td>{{ event.DateEnd | moment:'DD/MM/YYYY' }}</td>
                        <td>{{ event.LocationInfo.RegionInfo.Name[lang] }} - {{ event.LocationInfo.TvInfo.Name[lang] }}</td>
                        <td>
                            <div ng-repeat="thelang in event.HasLanguage">
                                <small>{{ thelang }}</small>
                            </div>
                        </td>
                        <td>
                            <div class="glyphicon glyphicon-ok" ng-show="event.Active"></div>
                            <div class="glyphicon glyphicon-remove" ng-show="event.Active == false"></div>
                        &nbsp;
                            <div class="glyphicon glyphicon-ok" ng-show="event.SmgActive" style="color:green"></div>
                            <div class="glyphicon glyphicon-remove" ng-show="event.SmgActive == false" style="color:red"></div>
                        </td>
                          <td>
                             <div ng-repeat="publishchannel in event.PublishedOn">
                                <small>{{ publishchannel }}</small>
                            </div>
                        </td>
                        <td>
                            <div class="btn-group-vertical">
                                @if (@ViewBag.globalmodify == true || @ViewBag.eventmodify == true || @ViewBag.globaldetail == true)
                                {
                                    <a ng-click="editevent(event)" class="btn btn-small btn-default" style="width:70px;">edit</a>
                                }
                                @if (@ViewBag.globaldelete == true || @ViewBag.eventdelete == true)
                                {
                                    <a ng-click="deleteevent(event.Id)" class="btn btn-small btn-danger" style="width:70px;">delete</a>
                                }
                                @if (@ViewBag.globalupdate == true || @ViewBag.eventupdate == true)
                                {
                                    <a ng-click="updateevent(event.Id)" class="btn btn-responsive btn-warning" style="width:70px;">Update</a>
                                }
                                <a ng-click="showInfoModal(event)" class="btn btn-responsive btn-success" style="width:70px;">Detail</a>
                            </div>
                        </td>
                    </tr>
                </tbody>
            </table>

            <div class="form-inline">
