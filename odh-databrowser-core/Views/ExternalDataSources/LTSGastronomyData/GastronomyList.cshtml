                                <img data-ng-src="{{slide.ImageUrl + '&H=320'}}" style="margin:auto;max-width:420px" class="img-rounded">
                                <div class="carousel-caption">
                                    <h4>{{slide.ImageTitle[lang]}}</h4>
                                    <p>{{slide.ImageDesc[lang]}}</p>
                                </div>
                            </slide>
                        </carousel>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div style="max-width:420px;height:320px">
                        <leaflet lf-center="leafletcoords" max-width="420px" height="320px"></leaflet>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">

                    <br />
                    <h2>{{ gastronomy.ContactInfos[lang].CompanyName }}</h2>
                    <h4>{{ gastronomy.LocationInfo.TvInfo.Name[lang] }} - {{ gastronomy.LocationInfo.RegionInfo.Name[lang] }}</h4>

                    <p ng-show="gastronomy.Detail[lang].IntroText != '' && gastronomy.Detail[lang].IntroText != null">
                        <strong>Introtext</strong><br />
                        {{ gastronomy.Detail[lang].IntroText }}
                    </p>
                    <p ng-show="gastronomy.Detail[lang].BaseText != '' && gastronomy.Detail[lang].BaseText != null">
                        <strong>Basistext</strong>
                    </p>
                    <p ng-show="gastronomy.Detail[lang].BaseText != '' && gastronomy.Detail[lang].BaseText != null" ng-bind-html="gastronomy.Detail[lang].BaseText"></p>

                    <p ng-show="gastronomy.Detail[lang].GetThereText != '' && gastronomy.Detail[lang].GetThereText != null">
                        <strong>Anfahrtstext</strong><br />
                        {{ gastronomy.Detail['de'].GetThereText }}
                    </p>
                    <p ng-show="gastronomy.Detail[lang].AdditionalText != '' && gastronomy.Detail[lang].AdditionalText != null">
                        <strong>Zusatztext</strong><br />
                        {{ gastronomy.Detail['de'].AdditionalText }}
                    </p>

                    <p ng-show="gastronomy.OperationSchedule != '' && gastronomy.OperationSchedule != null">
                        <strong>Season</strong><br />
                        <div ng-repeat=" operationschedule in gastronomy.OperationSchedule">
                            <strong ng-show="operationschedule.OperationscheduleName[lang] != ''">{{ operationschedule.OperationscheduleName[lang] }}</strong>
                            {{ operationschedule.Start | moment:'DD/MM/YYYY' }} -  {{ operationschedule.Stop | moment:'DD/MM/YYYY' }}
                            <div ng-repeat="operationscheduletime in operationschedule.OperationScheduleTime">

                                <div ng-show="operationscheduletime.State == 1">
                                    Closed on:
                                    {{ operationscheduletime.Start }} - {{ operationscheduletime.End }}

                                    <select class="form-control" id="selectOpeningtimecode" ng-model="operationscheduletime.Timecode" disabled>
                                        <option value="1">General Opening Time</option>
                                        <option value="2">Time range for warm meals</option>
                                        <option value="3">Time range for pizza</option>
                                        <option value="4">Time range for snack's</option>
                                    </select>

                                    <div class="btn-group-horizontal">
                                        <label class="btn btn-default" ng-model="operationscheduletime.Monday" btn-checkbox>Monday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Tuesday" btn-checkbox>Tuesday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Wednesday" btn-checkbox>Wednesday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Thuresday" btn-checkbox>Thursday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Friday" btn-checkbox>Friday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Saturday" btn-checkbox>Saturday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Sunday" btn-checkbox>Sunday</label>
                                    </div>


                                </div>
                                <div ng-show="operationscheduletime.State == 2">
                                    Opened:
                                    {{ operationscheduletime.Start }} - {{ operationscheduletime.End }}

                                    <select class="form-control" id="selectOpeningtimecode" ng-model="operationscheduletime.Timecode" disabled>
                                        <option value="1">General Opening Time</option>
                                        <option value="2">Time range for warm meals</option>
                                        <option value="3">Time range for pizza</option>
                                        <option value="4">Time range for snack's</option>
                                    </select>
                                    <div class="btn-group-horizontal">
                                        <label class="btn btn-default" ng-model="operationscheduletime.Monday" btn-checkbox>Monday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Tuesday" btn-checkbox>Tuesday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Wednesday" btn-checkbox>Wednesday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Thuresday" btn-checkbox>Thursday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Friday" btn-checkbox>Friday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Saturday" btn-checkbox>Saturday</label>
                                        <label class="btn btn-default" ng-model="operationscheduletime.Sunday" btn-checkbox>Sunday</label>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </p>

                    <ul class="list-group" ng-show="gastronomy.CategoryCodes != null">
                        <li class="list-group-item"><strong>Category</strong></li>
                        <li class="list-group-item" ng-repeat="gastrocodes in gastronomy.CategoryCodes">
                            <p>{{ gastrocodes.Shortname }}</p>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <table class="table table-bordered table-striped">
                        <tr>
                            <th colspan="2">Contactinfo</th>
                        </tr>
                        <tr>
                            <td>
                                Organisation
                            </td>
                            <td>
                                {{ gastronomy.ContactInfos[lang].CompanyName }}
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Adresse
                            </td>
                            <td>
                                {{ gastronomy.ContactInfos[lang].Address }}
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Ort
                            </td>
                            <td>
                                {{ gastronomy.ContactInfos[lang].ZipCode }} - {{ gastronomy.ContactInfos[lang].City }}
                            </td>
                        </tr>
                        <tr ng-show="activity.ContactInfos[lang].Surname != ''">
                            <td>
                                Ansprechpartner
                            </td>
                            <td>
                                {{ gastronomy.ContactInfos[lang].Surname }}&nbsp;{{ gastronomy.ContactInfos[lang].Givenname }}
                            </td>
                        </tr>
                        <tr ng-show="activity.ContactInfos[lang].Phonenumber != ''">
                            <td>
                                Tel / Fax
                            </td>
                            <td>
                                {{ gastronomy.ContactInfos[lang].Phonenumber }}<br />
                                {{ gastronomy.ContactInfos[lang].Faxnumber }}
                            </td>
                        </tr>
                        <tr>
                            <td>
                                E-Mail
                            </td>
                            <td>
                                {{ gastronomy.ContactInfos[lang].Email }}
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Web
                            </td>
                            <td>
                                {{ gastronomy.ContactInfos[lang].Url }}
                            </td>
                        </tr>
                    </table>
                </div>
                <div class="col-sm-6">
                    <ul class="list-group" ng-show="gastronomy.Facilities != null">
                        @*<li class="list-group-item" ng-repeat="facilities in gastronomy.Facilities | filter:{Language:'de'}">*@
                        <li class="list-group-item"><strong>Facilities</strong></li>
                        <li class="list-group-item" ng-repeat="facilities in gastronomy.Facilities">
                            <p>{{ facilities.Shortname }}</p>
                        </li>
                    </ul>
                    <ul class="list-group" ng-show="gastronomy.DishRates != null">
                        <li class="list-group-item"><strong>Dishrates</strong></li>
                        <li class="list-group-item" ng-repeat="dishrates in gastronomy.DishRates">
                            <p>{{ dishrates.Shortname }}</p>
                        </li>
                    </ul>
                    <ul class="list-group" ng-show="gastronomy.CapacityCeremony != null">
                        <li class="list-group-item"><strong>Ceremony Codes</strong></li>
                        <li class="list-group-item" ng-repeat="capacityceremony in gastronomy.CapacityCeremony">
                            <p>{{ capacityceremony.Shortname }}</p>
                        </li>
                    </ul>

                </div>

            </div>

        </div>

        <div class="modal-footer">
            <button class="btn btn-info" ng-click="cancelslidemodal()">Cancel</button>
        </div>
    </script>
</div>
@section Styles{
    <style type="text/css">
    </style>
}

