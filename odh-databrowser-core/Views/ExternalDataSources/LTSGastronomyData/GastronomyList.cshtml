        <div class="col-sm-9 col-xs-12 table-responsive">

            <table class="table table-striped table-condensed">
                <thead>
                    <tr>
                        <th style="width:160px">MainImage</th>
                        <th style="min-width: 80px;">Name</th>
                        <th style="min-width: 80px;">Fraction - Region - TV</th>
                        <th style="min-width: 80px;">active on source</th>
                        @*<th style="min-width: 80px;">ODH active</th>*@

                        <th style="width:20px;"> </th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="gastronomy in gastronomies | filter:query">
                        <td>
                            <div style="max-width:150px;min-height:75px">
                                <img class="img-thumbnail img-responsive" @*style="max-height:150px;max-width:150px"*@ ng-src="{{  gastronomy.ImageGallery[0] != NULL && gastronomy.ImageGallery[0].ImageUrl + '&W=150&H=150' || '../img/noimage.png' }}" @*ng-hide="activity.ImageGallery[0] == NULL"*@ title="{{ gastronomy.ImageGallery[0].ImageDesc['de']}}" ng-click="showInfoModal(gastronomy)" />
                            </div>
                        </td>
                        @*<td>{{ activity.Id }}</td>*@
                        <td>{{ gastronomy.ContactInfos[lang].CompanyName }}</td>
                        <td>{{ gastronomy.LocationInfo.DistrictInfo.Name[lang] }} - {{ gastronomy.LocationInfo.TvInfo.Name[lang] }} - {{ gastronomy.LocationInfo.RegionInfo.Name[lang] }}</td>
                        <td>
                            <div class="glyphicon glyphicon-ok" ng-show="gastronomy.Active"></div>
                            <div class="glyphicon glyphicon-remove" ng-show="gastronomy.Active == false"></div>
                        </td>
                        @*<td>
                            <div class="glyphicon glyphicon-ok" ng-show="gastronomy.SmgActive" style="color:green"></div>
                            <div class="glyphicon glyphicon-remove" ng-show="gastronomy.SmgActive == false" style="color:red"></div>
                        </td>*@
                        <td>
                            <div class="btn-group-vertical">
                                @if (@ViewBag.globalmodify == true || @ViewBag.gastromodify == true || @ViewBag.globaldetail == true)
                                {
                                    <a ng-click="editgastronomy(gastronomy)" class="btn btn-responsive btn-info" style="width:70px;">edit</a>
                                }
                                @if (@ViewBag.globaldelete == true || @ViewBag.gastrodelete == true)
                                {
                                    <a ng-click="deletegastronomy(gastronomy.Id)" class="btn btn-responsive btn-danger" style="width:70px;">delete</a>
                                }
                                @if (@ViewBag.globalupdate == true || @ViewBag.gastroupdate == true)
                                {
                                    <a ng-click="updategastronomy(gastronomy.Id)" class="btn btn-responsive btn-warning" style="width:70px;">Update</a>
                                }
                                <a ng-click="showInfoModal(gastronomy)" class="btn btn-responsive btn-success" style="width:70px;">Detail</a>
                            </div>
                        </td>
                    </tr>
                </tbody>
            </table>

            <div class="form-inline">
